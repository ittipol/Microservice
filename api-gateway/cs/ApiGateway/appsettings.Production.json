{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Secret": "uDnF3+6uGj+tyvqRrzfCqc1czsKOnW8m+xv7lnOBDzuIGIkjphTa6aGjuQbbMQ79EAI22YU7bTfhTQzyqKMgBQ=="
  },
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "MaxRequestBodySize": 1000000,
        "Match": {
          "Path": "auth/{**catch-all}",
          "Methods": [
            "GET",
            "POST"
          ]
        },
        "Transforms": [
          {
            "PathPattern": "{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "LoadBalancingPolicy": "RoundRobin",
        "Destinations": {
          "cluster1": {
            "Address": "http://auth-service-server.auth-service.svc.cluster.local:3000"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": "true",
            "Interval": "00:00:10",
            "Timeout": "00:00:10",
            "Policy": "ConsecutiveFailures",
            "Path": "/health"
          }
        },
        "HttpClient": {
          "SSLProtocols": "Tls13",
          "DangerousAcceptAnyServerCertificate": false,
          "MaxConnectionsPerServer": 1024,
          "EnableMultipleHttp2Connections": true,
          "RequestHeaderEncoding": "utf-8",
          "ResponseHeaderEncoding": "utf-8"
        },
        "HttpRequest": {
          "ActivityTimeout": "00:00:30"
        }
      }
    }
  }
}